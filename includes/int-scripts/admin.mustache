<script id="bm_press_model_table" type="text/html">
<table class="table table-striped table-hover">
	<thead>
		<th>Designed for</th>
		<th>Iteration</th>
		<th>Created on</th>
		<th>Changed on</th>
		<th>Designed by</th>
		<th>Action</th>
	</thead>
	<tbody>
		{{# models }}
		<tr>
			<td><a href="#model/{{ id }}">{{ designed_for }}</a></td>
			<td>{{ iteration }}</td>
			<td>{{ time_created }}</td>
			<td>{{ time_saved }}</td>
			<td>{{ designed_by }} </td>
			<td>
				<div class="btn-group">
					<button class="btn"
						onclick="window.location.href='#model/{{ id }}';"
						><i class="icon-folder-open"></i> Open</button>
					<button class="btn dropdown-toggle" data-toggle="dropdown">
						<span class="caret"></span>
					</button>
					<ul class="dropdown-menu pull-right">
						<li><a tabindex="-1"
							href="javascript: bm_press.list.iterate('{{ id }}');"
							><i class="icon-repeat"></i> Iterate</a></li>
						<li><a tabindex="-1"
							href="javascript: bm_press.modal.list_shortcode('{{ id }}');"
							><i class="icon-share"></i> Shortcode</a></li>
						<li class="divider"></li>
						<li><a tabindex="-1"
							href="javascript: bm_press.modal.list_delete('{{ id }}');"
							><i class="icon-trash"></i> Delete</a></li>
					</ul>

				</div>

				{{> bm_press_list_modal }}
			</td>
		</tr>
		{{/ models }}
	</tbody>
</table>

</script>



