<script id="bm_press_hypo_p" type="text/html" class="partial">

<div class="bm_press_hypo alert dropdown"
	id="{{ hypo_luid }}"
	state="{{ state }}">

	<button class="btn btn-mini dropdown-toggle pull-right"
		data-toggle="dropdown" on>
		<span class="caret"></span>
	</button>
	<ul class="dropdown-menu pull-right">
		<!--<li class="nav-header">Set state:</li>-->
		<li><a class="alert alert-warning" tabindex="-1"
			href="javascript: bm_press.hypo.set_state('{{ hypo_luid }}','U');"
			><i class="icon-pause"></i> Pending</a></li>
		<li><a class="alert alert-info" tabindex="-1"
			href="javascript: bm_press.hypo.set_state('{{ hypo_luid }}','A');"
			><i class="icon-play"></i> Active/Highlight</a></li>
		<li><a class="alert alert-success" tabindex="-1"
			href="javascript: bm_press.hypo.set_state('{{ hypo_luid }}','C');"
			><i class="icon-ok"></i> Valid</a></li>
		<li><a class="alert alert-error" tabindex="-1"
			href="javascript: bm_press.hypo.set_state('{{ hypo_luid }}','D');"
			><i class="icon-ban-circle"></i> Invalid</a></li>

		<li class="divider"></li>
		<li><a tabindex="-1"
			href="javascript: bm_press.hypo.edit('{{ hypo_luid }}');"
			><i class="icon-pencil"></i> Edit text</a></li>
		<li class="divider"></li>
		<li><a tabindex="-1"
			href="javascript: bm_press.hypo.delete('{{ hypo_luid }}');"
			><i class="icon-trash"></i> Delete</a></li>
	</ul>
	<p class="hypo_title" id="{{ hypo_luid }}_title">{{ title }}</p>

</div>

</script>
<script id="bm_press_hypo" type="text/html">
	{{> bm_press_hypo_p }}
</script>


