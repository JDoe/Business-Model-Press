			<td><input id="outputSavedOn" disabled class="input-large" type="text"
				value="{{ time_saved }}"></td>
		</tr>
		<tr>
			<td>Designed by</td>
			<td><input id="inputDesignedBy" disabled
				class="input-large" type="text"
				value="{{ designed_by }}" onchange="bm_press.model.has_changed();"></td>
		</tr>
	</tbody>
</table>

</script>

