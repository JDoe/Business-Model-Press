<script id="bm_press_model_modals" type="text/html" class="partial">

<div class="modal hide fade" id="close_without_saving">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h3>Close without saving?</h3>
  </div>
  <div class="modal-body">
    <p>Your business model has changed since last time it was saved.
    Do you want to save changes before closing?</p>
  </div>
  <div class="modal-footer">
  <a href="javascript:bm_press('#close_without_saving').modal('hide');" class="btn">Cancel</a>
    <a href="javascript:bm_press.model.confirmed_close();" class="btn">Close without saving</a>
    <a href="javascript:bm_press.model.save({then_close: true});" class="btn btn-primary saveButton">Save changes and close</a>
  </div>
</div>

<div class="modal hide fade" id="delete_model">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h3>Are you sure you want to delete this business model?</h3>
  </div>
  <div class="modal-footer">
  	<a href="javascript:bm_press('#delete_model').modal('hide');" class="btn">Cancel</a>
    <a href="javascript:bm_press.model.confirmed_delete();" class="btn btn-primary deleteButton"><i class="icon-trash icon-white"></i> Delete model</a>
  </div>
</div>

</script>


<script id="bm_press_list_modal" type="text/html" class="partial">

<div class="modal hide fade" id="delete_model_{{ id }}">
	<div class="modal-header">
		<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
		<h3>Are you sure you want to delete this business model?</h3>
	</div>
	<div class="modal-body">
		<table class="table table-striped table-hover">
			<thead>
				<th>Designed for</th>
				<th>Iteration</th>
				<th>Created on</th>
				<th>Changed on</th>
				<th>Designed by</th>
			</thead>
			<tbody>
				<tr>
					<td>{{ designed_for }}</td>
					<td>{{ iteration }}</td>
					<td>{{ time_created }}</td>
					<td>{{ time_saved }}</td>
					<td>{{ designed_by }} </td>
				</tr>
			</tbody>
		</table>
 	</div>
 	<div class="modal-footer">
	 	<a href="javascript:bm_press('#delete_model_{{ id }}').modal('hide');"
	 		class="btn">Cancel</a>
	 	<a href="javascript:bm_press.list.confirmed_delete('{{ id }}');"
	 		class="btn btn-primary deleteButton"><i class="icon-trash icon-white"></i> Delete model</a>
 	</div>
</div>

<div class="modal hide fade" id="shortcode_model_{{ id }}">
	<div class="modal-header">
		<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
		<h3>Shortcode for use in posts</h3>
	</div>
	<div class="modal-body">
		{{> bm_press_shortcode }}
 	</div>
 	<div class="modal-footer">
	 	<a href="javascript:bm_press('#shortcode_model_{{ id }}').modal('hide');"
	 		class="btn">Close</a>
 	</div>
</div>

</script>